{
  "name": "shipit",
  "description": "A team of 12 specialist AI agents for building products â€” from idea to shipped software. Uses Agent Teams for parallel execution and hybrid memory for self-improvement.",
  "version": "2.0.0",
  "author": {
    "name": "Claire Donald"
  },
  "homepage": "https://github.com/cla1redonald/shipit-v2",
  "license": "MIT",
  "keywords": ["agents", "product", "development", "automation", "team", "parallel", "orchestration"],
  "agents": [
    "agents/orchestrator.md",
    "agents/researcher.md",
    "agents/strategist.md",
    "agents/pm.md",
    "agents/architect.md",
    "agents/designer.md",
    "agents/engineer.md",
    "agents/devsecops.md",
    "agents/reviewer.md",
    "agents/qa.md",
    "agents/docs.md",
    "agents/retro.md"
  ],
  "skills": [
    "skills/prd-review",
    "skills/code-review",
    "skills/prd-threads"
  ],
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          { "type": "command", "command": "node hooks/pre-push-check.js" },
          { "type": "command", "command": "node hooks/security-scan.js" }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          { "type": "command", "command": "node hooks/post-completion.js" }
        ]
      }
    ]
  }
}
